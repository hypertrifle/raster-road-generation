{
  luxe:{
    window: {
      width: 1280,
      height: 700,
      title:'course',
      fullscreen:false,
      resizable:true,
      borderless:false
    }
  },

  project : {
    name : 'course_gen',
    version : '0.0.1',
    author : 'Rich Searle',

    app : {
      name : 'luxe_empty',
      package : 'com.hypertrifle.course'
    },

    build : {
      dependencies : {
        luxe : '*',
      },

      post : {
           name : 'log-haxelib-version',
           desc : 'will save a log of the current installed haxelib versions on successful build',
           script : 'hooks/log-haxelib-versions.js',
       }
    },

    files : {
      assets : 'assets/'
    }
  }
}
